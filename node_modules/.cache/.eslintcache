Land = {e.area}\n                                        landName={e.name}\n                                        region={e.region}\n                                        capital={e.capital}\n                                        flag={e.flag}\n                                        languages={e.languages}\n                                        nativeName={e.nativeName}\n                                        area={e.area}\n                                        population={e.population}\n                                      />)\n                        })\n                        } </div>\n                        </>} />\n              \n\n              <Route path = '/continent/' \n                     element = {<div className='home-items'> <FiveContinents/></div>}/>\n              <Route path = '/continent/:name' \n                     element = {<Continent countries={countries}/>}/>\n\n              {/* <Route path = '/search/:search'\n                     element = {<div className='home-items'><Search countries={countries}/></div>}/>\n              <Route path = '/search/'\n                     element = {<div className='home-items'><Search/></div>}/> */}\n\n              <Route path = '/visited/'\n                     element = {<div className='home-items'>Visited lands here</div>}/>\n              <Route path = '/to_visit/'\n                     element = {<div className='home-items'>I wish to visit these Lands</div>}/>\n                                    \n              </Routes>\n          </div>\n         </ThemeContextProvider>\n        </div>\n    </Router>\n  );\n}\n\nexport default App;\n\n",{"ruleId":"68","severity":1,"message":"69","line":2,"column":8,"nodeType":"70","messageId":"71","endLine":2,"endColumn":15},{"ruleId":"68","severity":1,"message":"72","line":12,"column":12,"nodeType":"70","messageId":"71","endLine":12,"endColumn":16},{"ruleId":"68","severity":1,"message":"73","line":1,"column":17,"nodeType":"70","messageId":"71","endL